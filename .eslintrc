{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx":true,
      "experimentalObjectRestSpread": true
    }
  },
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ]
}